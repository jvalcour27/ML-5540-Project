# Define features (X) and new target(s) (y_recode) for the training set\n","X_train = df_train.drop(columns=columns_to_drop_from_X)\n","y_train_recode = df_train[bar_recode_cols]\n","\n","# Define features (X) and new target(s) (y_recode) for the testing set\n","X_test = df_test.drop(columns=columns_to_drop_from_X)\n","y_test_recode = df_test[bar_recode_cols]\n","\n","# Define features (X) and new target(s) (y_recode) for the validation set\n","X_val = df_validation.drop(columns=columns_to_drop_from_X)\n","y_val_recode = df_validation[bar_recode_cols]\n","\n","print(f\"Data split based on training year <= {training_cutoff_year}, and remaining split equally into test and validation:\")\n","print(\"Shape of X_train:\", X_train.shape)\n","print(\"Shape of y_train_recode:\", y_train_recode.shape)\n","print(\"Shape of X_test:\", X_test.shape)\n","print(\"Shape of y_test_recode:\", y_test_recode.shape)\n","print(\"Shape of X_val:\", X_val.shape)\n","print(\"Shape of y_val_recode:\", y_val_recode.shape)"],"execution_count":70,"outputs":[{"output_type":"stream","name":"stdout","text":["Data split based on training year <= 1999, and remaining split equally into test and validation:\n","Shape of X_train: (5232, 165)\n","Shape of y_train_recode: (5232, 5)\n","Shape of X_test: (565, 165)\n","Shape of y_test_recode: (565, 5)\n","Shape of X_val: (565, 165)\n","Shape of y_val_recode: (565, 5)\n"]}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"aea9e0ab","executionInfo":{"status":"ok","timestamp":1765195203857,"user_tz":300,"elapsed":6,"user":{"displayName":"Jeannine Valcour","userId":"16417877083702459285"}},"outputId":"640a8070-a7e8-4ff4-fbe4-580cc01d0b5b"},"source":["print(\"Columns in X_train:\")\n","print(X_train.columns.tolist())"],"execution_count":71,"outputs":[{"output_type":"stream","name":"stdout","text":["Columns in X_train:\n","['Year', 'NUMBER_OF_Countries', 'Issue_Cat_Border_Issues', 'Issue_Cat_Economic_Development', 'Issue_Cat